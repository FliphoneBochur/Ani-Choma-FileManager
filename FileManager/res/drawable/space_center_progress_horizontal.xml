<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ffebebeb" android:endColor="#ffebebeb" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffebebeb" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ffffc107" android:endColor="#ffffc107" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffffc107" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ffffc107" android:endColor="#ffffc107" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffffc107" />
            </shape>
        </clip>
    </item>
</layer-list>